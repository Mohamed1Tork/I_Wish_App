            <TextField fx:id="emailTF" layoutX="319.0" layoutY="282.0" prefHeight="36.0" prefWidth="214.0" promptText="   Email" style="-fx-border-color: #2B3467; -fx-border-radius: 20,20,20,20; -fx-background-radius: 20,20,20,20;">
